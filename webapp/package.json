{
    "name": "sunday-service-vr",
    "version": "0.2.0",
    "private": true,
    "homepage": "https://sunday-service-vr.web.app",
    "scripts": {
        "start": "npm-run-all --parallel start:webapp start:emulator",
        "start:webapp": "vite",
        "start:emulator": "firebase emulators:start --only functions,firestore --import ../firebase-emulator-data --export-on-exit --inspect-functions",
        "force-kill-emulators": "npx kill-port 4000 8080 8085",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "test": "vitest --globals"
    },
    "dependencies": {
        "dayjs": "^1.11.10",
        "discord-api-types": "^0.37.120",
        "firebase-functions": "^5.0.1",
        "js-cookie": "^3.0.5",
        "react": "^18.2.0",
        "react-datepicker": "^4.8.0",
        "react-dom": "^18.2.0",
        "react-feather": "^2.0.10",
        "react-hot-toast": "^2.4.1",
        "react-router": "^6.22.3",
        "react-router-bootstrap": "^0.26.3",
        "react-router-dom": "^6.22.3",
        "ulid": "^2.3.0",
        "upgrade": "^1.1.0",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.4.5",
        "@testing-library/react": "^15.0.7",
        "@types/js-cookie": "^3.0.6",
        "@types/node": "^20.12.12",
        "@types/react": "^18.3.2",
        "@types/react-datepicker": "^4.4.2",
        "@types/react-dom": "^18.3.0",
        "@types/react-router-bootstrap": "^0.26.6",
        "@typescript-eslint/eslint-plugin": "^7.9.0",
        "@typescript-eslint/parser": "^7.9.0",
        "@vitejs/plugin-react": "^4.2.1",
        "bootstrap": "^5.3.3",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "firebase": "^11.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jsdom": "^24.0.0",
        "npm-run-all": "^4.1.5",
        "react-bootstrap": "^2.10.2",
        "react-bootstrap-typeahead": "^6.3.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "uuid": "^11.1.0",
        "vite": "^5.2.11",
        "vite-plugin-svgr": "^4.2.0",
        "vitest": "^1.6.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.6.1"
    }
}
